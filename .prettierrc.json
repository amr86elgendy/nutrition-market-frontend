{
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"endOfLine": "auto",
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "avoid",
	"singleAttributePerLine": true,
	"plugins": ["prettier-plugin-tailwindcss"]
}
